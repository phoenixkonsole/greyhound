prefix=/SDK/local/clib2
exec_prefix=/SDK/local/clib2
libdir=${exec_prefix}/lib
includedir=/SDK/local/common/include
supported_protocols="DICT FILE FTP FTPS GOPHER GOPHERS HTTP HTTPS IMAP IMAPS IPFS IPNS MQTT POP3 POP3S RTMP RTSP SMB SMBS SMTP SMTPS TELNET TFTP WS WSS"
supported_features="alt-svc AsynchDNS HSTS HTTPS-proxy libz NTLM SSL threadsafe TLS-SRP"

Name: libcurl
#URL: https://curl.se/
Description: Library to transfer files with HTTP, FTP, etc.
Version: 8.13.0
Requires: zlib,openssl,librtmp
Requires.private: zlib,openssl,librtmp
Libs: -L${libdir} -lcurl -lrtmp -lm -lssl -lcrypto -lnet -lz -lpthread
Libs.private: -L/SDK/local/clib2/lib -lrtmp -lm -lssl -lcrypto -lnet -lz -lpthread
Cflags: -I${includedir} -DCURL_STATICLIB
Cflags.private: -DCURL_STATICLIB
